define <%= module_name %>::controls::<%= @entry.control_name %>
{
  <%= module_name %>_setup { "<%= @entry.control_name %> on ${title}":
    ensure => 'present',
  }

  alert('Specified CIS control is not yet automatically enforced.')
}
